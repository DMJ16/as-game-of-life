{
  "name": "as-game-of-life",
  "version": "0.0.1",
  "description": "AssemblyScript/React reworking of Conway's Game of Life.",
  "scripts": {
    "asbuild:untouched": "asc ./node_modules/as-bind/lib/assembly/as-bind.ts assembly/index.as.ts --target debug",
    "asbuild:optimized": "asc ./node_modules/as-bind/lib/assembly/as-bind.ts assembly/index.as.ts --target release",
    "asbuild": "npm run asbuild:untouched && npm run asbuild:optimized",
    "build": "parcel build web/dist/index.html",
    "start": "parcel web/dist/index.html"
  },
  "keywords": [
    "assemblyscript",
    "react",
    "conway's game of life",
    "webassembly"
  ],
  "author": "david josephs",
  "license": "ISC",
  "dependencies": {
    "@assemblyscript/loader": "^0.14.12",
    "@types/node": "^14.11.2",
    "as-bind": "^0.4.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "use-as-bind": "^0.1.0"
  },
  "devDependencies": {
    "@types/react": "^16.9.49",
    "@types/react-dom": "^16.9.8",
    "assemblyscript": "^0.14.12",
    "parcel": "^2.0.0-beta.1",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.3"
  }
}
